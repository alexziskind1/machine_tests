{
  "schema_version": 3,
  "headers": {
    "Content-Type": "application/json"
  },
  "_comment_hardware_section": "Physical machine characteristics only (no model/backend/runtime here). VRAM allocation can be dynamic or static",
  "hardware": {
    "make": "NVIDIA",
    "device_model": "DGX Spark",
    "code": "dgx_spark",  
    "cpu": "GB10",
    "memory_gb": 128,
    "is_igpu": true,
    "gpu": "GB10",
    "vram_allocation": "dynamic",
    "vram_gb": 128,
    "notes": "NVIDIA DGX Spark"
  },
  "environment": {
    "os": "DGX OS"
  },
  "_comment_target_section": "Target inference configuration (model + runtime stack)",
  "target": {
    "llm_url": "http://127.0.0.1:8000/v1/chat/completions",
    "model": "Qwen/Qwen3-Coder-30B-A3B-Instruct",
    "quant": "bf16",
    "backend": "cuda",
    "runtime": "vllm"
  },
  "request_timeout": 1000,
  "output_csv": "results.csv",

  "_comment_results_organization": "Results are automatically organized in nested folders: /results/<model>/<quantization>/<hardware_code>/",
  "_comment_hardware_override": "Use --hardware argument to specify hardware manually; sets hardware code in folder path",
  
  "_comment_generation_params": "Parameters to control response generation for consistent benchmarking",
  "max_tokens": 512,
  "temperature": 0.1,
  
  "_comment_statistical_params": "Parameters for statistical reliability testing",
  "iterations_per_prompt": 5,
  "outlier_threshold": 2.0,
  "warmup_iterations": 2,
  "cooldown_between_iterations": 2,
  "cooldown_between_prompts": 5,
  
  "_comment_reliability": "Reliability thresholds for analysis",
  "max_acceptable_cv": 20.0,
  "min_success_rate": 80.0
}
